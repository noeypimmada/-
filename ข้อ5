numbers = []  # สร้างลิสต์ว่าง

# รับค่าจำนวนเต็ม 5 ตัว
count = 0
while count < 5:
    num = int(input("ป้อนตัวเลขจำนวนเต็ม: "))
    numbers.append(num)
    count += 1

# คำนวณผลรวม
total = sum(numbers)

# คำนวณค่าเฉลี่ย
average = total / len(numbers)

# แสดงผลลัพธ์
print("ผลรวม =", total)
print("ค่าเฉลี่ย =", average)
